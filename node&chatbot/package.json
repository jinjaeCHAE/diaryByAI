{
  "name": "@ibm-watson/assistant-intermediate",
  "description": "A simple Node.js based web app which shows how to use the Watson Assistant API to recognize user intents.",
  "version": "0.1.1",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test-integration": "casperjs test ./test/integration/test.*.js",
    "test-integration-runner": "NODE_ENV=test node casper-runner.js",
    "test": "npm run lint && npm run test-integration-runner",
    "test-unit": "jest test/unit --coverage",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/watson-developer-cloud/assistant-intermediate.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ibm-watson": "^5.1.0",
    "mysql": "^2.18.1",
    "process": "^0.11.10",
    "python": "0.0.4",
    "python-shell": "^2.0.3",
    "urlencode": "^1.1.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "engineStrict": true,
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "casperjs": "^1.1.4",
    "codecov": "^3.6.1",
    "eslint": "^6.6.0",
    "jest": "^26.6.3",
    "phantomjs-prebuilt": "^2.1.16",
    "supertest": "^4.0.2"
  }
}
